  "figmaPlugin": {
    "name": "Figma Chat",
    "id": "742073255743594050",
    "api": "1.0.0",
    "main": "code.js",
    "ui": "ui.html",
    "enableProposedApi": false,
    "editorType": [
      "figma",
      "figjam"
    ],
    "permissions": [
      "currentuser"
    ],
    "menu": [
      {
        "name": "Open Chat",
        "command": "open"
      },
      {
        "separator": true
      },
      {
        "name": "Reset Chat",
        "command": "reset"
      }
    ],
    "relaunchButtons": [
      {
        "command": "open",
        "name": "Open Figma-Chat"
      },
      {
        "command": "relaunch",
        "name": "Send selection to chat",
        "multipleSelection": true
      }
    ]
